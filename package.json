{
  "name": "nailshop-vue-typescript",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint \"{src,tests}/**/*.{ts,tsx,vue}\"",
    "format": "prettier --write \"src/**/*.{ts,tsx,vue,scss,css}\"",
    "test:unit": "vitest run",
    "test:e2e": "cypress run --config-file tests/e2e/cypress.config.ts",
    "test": "vitest"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "axios": "^1.7.9",
    "pinia": "^2.2.6",
    "vue": "^3.5.22",
    "vue-router": "^4.4.5",
    "vuetify": "^3.7.4"
  },
  "devDependencies": {
    "@types/node": "^24.6.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^2.1.5",
    "@vue/eslint-config-prettier": "^10.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "cypress": "^13.13.3",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.32.0",
    "jsdom": "^25.0.1",
    "msw": "^2.6.0",
    "prettier": "^3.4.1",
    "sass": "^1.81.0",
    "typescript": "~5.9.3",
    "vite": "^7.1.7",
    "vite-plugin-vuetify": "^2.0.3",
    "vitest": "^2.1.5",
    "vue-tsc": "^3.1.0"
  }
}
